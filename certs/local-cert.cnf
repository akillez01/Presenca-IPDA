[req]
default_bits = 2048
distinguished_name = dn
req_extensions = v3_req
prompt = no

[dn]
C = BR
ST = AM
L = Manaus
O = IPDA
OU = Presenca
CN = localhost

[v3_req]
subjectAltName = @alt_names
keyUsage = digitalSignature, keyEncipherment
extendedKeyUsage = serverAuth

[alt_names]
DNS.1 = localhost
IP.1 = 127.0.0.1
IP.2 = 192.168.0.17
